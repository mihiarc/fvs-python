{
  "section": "6.0",
  "title": "Regeneration",
  "description": "The SN variant contains a partial establishment model which may be used to input regeneration and ingrowth into simulations. A more detailed description of how the partial establishment model works can be found in section 5.4.5 of the Essential FVS Guide (Dixon 2002).",
  "methodology": {
    "approach": "Partial establishment model",
    "reference": "Section 5.4.5 of the Essential FVS Guide (Dixon 2002)",
    "purpose": "Simulate stand establishment from bare ground, or to bring seedlings and sprouts into a simulation with existing trees",
    "automatic_sprouting": "Sprouts are automatically added following harvest or burning of known sprouting species"
  },
  "equations": {
    "root_suckering_logic": {
      "equation_id": "6.0.1",
      "description": "For root suckering hardwood species - determines number of sprout records",
      "conditions": [
        "DSTMPi ≤ 5: NUMSPRC = 1",
        "5 < DSTMPi ≤ 10: NUMSPRC = NINT(-1.0 + 0.4 * DSTMPi)",
        "DSTMPi > 10: NUMSPRC = 3"
      ]
    },
    "trees_per_acre": {
      "equation_id": "6.0.2",
      "formula": "TPAs = TPAi * PS",
      "description": "Trees-per-acre represented by each sprout record"
    },
    "sprouting_probability_general": {
      "equation_id": "6.0.3",
      "formula": "PS = [1 / (1 + exp(-(b0 + b1 * DSTMPi)))]",
      "description": "General sprouting probability equation"
    },
    "sprouting_probability_alt1": {
      "equation_id": "6.0.4",
      "formula": "PS = ((57.3 - 0.0032 * (DSTMPi^3)) / 100)",
      "description": "Alternative sprouting probability equation (Johnson 1975)"
    },
    "sprouting_probability_alt2": {
      "equation_id": "6.0.5",
      "formula": "PS = (1 / (1 + exp(-(2.3656 - 0.2781 * (DSTMPi / 0.7801)))))",
      "description": "Alternative sprouting probability equation (Johnson 1977)"
    },
    "sprouting_probability_alt3": {
      "equation_id": "6.0.6",
      "formula": "PS = (1 / (1+exp(-(-2.8058 + 22.6839 * (1 / ((DSTMPi / 0.7788) - 0.4403))))))",
      "description": "Alternative sprouting probability equation (Johnson 1977)"
    }
  },
  "variables": {
    "DSTMPi": "diameter at breast height of the parent tree",
    "NUMSPRC": "number of sprout tree records",
    "NINT": "rounds the value to the nearest integer",
    "TPAs": "trees per acre represented by each sprout record",
    "TPAi": "trees per acre removed/killed represented by the parent tree",
    "PS": "sprouting probability (see table 6.0.2)",
    "b0": "species-specific coefficient",
    "b1": "species-specific coefficient"
  },
