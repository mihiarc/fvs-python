{
  "title": "Crown Ratio Relationships for SN Southern Variant",
  "description": "Complete crown ratio equations and coefficients used in the SN variant of FVS",
  "equations": {
    "dead_trees": {
      "description": "Crown ratio missing on dead trees",
      "equation_4_3_1_1": {
        "dbh_less_than_24": "X = 0.70 - 0.40/24.0 * DBH",
        "dbh_greater_than_24": "X = 0.30"
      },
      "equation_4_3_1_2": {
        "formula": "CR = 1 / (1 + exp(X + N(0,SD)))",
        "bounds": "0.05 < CR < 0.95"
      }
    },
    "live_trees_weibull": {
      "description": "Weibull-based crown model for live trees (Dixon 1985, 2002)",
      "average_crown_ratio_equations": {
        "4_3_1_3": "ACR = exp[d0 + (d1 * ln(RELSDI)) + (d2 * RELSDI)]",
        "4_3_1_4": "ACR = exp[d0 + (d1 * ln(RELSDI))]",
        "4_3_1_5": "ACR = d0 + (d2 * RELSDI)",
        "4_3_1_6": "ACR = d0 + (d1 * log10(RELSDI))",
        "4_3_1_7": "ACR = RELSDI / ((d0 * RELSDI) + d1)"
      },
      "weibull_parameters": {
        "4_3_1_8": {
          "A": "a0",
          "B": "b0 + b1 * ACR (bounded > 3.0)",
          "C": "c (bounded > 2.0)"
        }
      },
      "individual_tree_crown_ratio": {
        "4_3_1_9": "Y = A + B(-ln(1-X))^(1/C)"
      },
      "scale_factor": {
        "4_3_1_10": "SCALE = 1 - 0.00167 * (CCF - 100) (bounded 0.3 < SCALE < 1.0)"
      }
    },
    "regeneration": {
      "description": "Crown ratios for newly established trees",
      "equation_4_3_3_1": {
        "formula": "CR = 0.89722 - 0.0000461 * PCCF + RAN",
        "bounds": "0.2 < CR < 0.9"
      }
    }
  },
